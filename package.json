{
    "name": "@gopowerteam/svelte-modal",
    "version": "0.1.0",
    "files": [
        "dist",
        "README.md"
    ],
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./dist/": "./dist/"
    },
    "types": "./dist/index.d.ts",
    "author": {
        "name": "zhuchentong",
        "email": "2037630@gmail.com"
    },
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "build": "npm run check && vite build && npm run generate:type",
        "generate:type": "tsc --p tsconfig.dts.json --declaration --emitDeclarationOnly --esModuleInterop --outDir dist"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.14",
        "@tsconfig/svelte": "^2.0.1",
        "@types/node": "^16.4.3",
        "prettier": "^2.3.1",
        "prettier-plugin-svelte": "^2.3.1",
        "stylus": "^0.54.8",
        "svelte": "^3.37.0",
        "svelte-check": "^2.1.0",
        "svelte-preprocess": "^4.7.2",
        "tslib": "^2.2.0",
        "typescript": "^4.3.5",
        "vite": "^2.4.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gopowerteam/package.svelte-modal.git"
    },
    "bugs": {
        "url": "https://github.com/gopowerteam/package.svelte-modal/issues"
    },
    "homepage": "https://github.com/gopowerteam/package.svelte-modal",
    "license": "MIT",
    "peerDependencies": {
        "svelte": "^3.0.0"
    }
}
